{
    "version": 2,
    "builds": [
      {
        "src": "cgdaServer/**/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "cgdaServer/api/$1.py"
      },
      {
        "src": "/(.*)",
        "dest": "cgdaServer/manage.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "cgdaServer.settings",
      "VERCEL": "1"
    }
  }
  